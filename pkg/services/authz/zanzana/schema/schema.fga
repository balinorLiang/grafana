model
  schema 1.1

type instance

type user

type org
  relations
    define instance: [instance]
    define member: [user]

type role
  relations
    define assignee: [user, team#member, role#assignee]
    define instance: [instance]
    define org: [org]

type team
  relations
    define admin: [user]
    define member: [user] or admin
    define org: [org]
